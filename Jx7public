local a = {['Mex_ej']=true,['Fresade80']=true,['DTH_KILLS']=true}
local b = game:GetService('Players')
local c = b.LocalPlayer
if not a[c.Name] then c:Kick('No estas en WhiteList para usar este script') end
local d = loadstring(game:HttpGet('https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library',true))()
local e = c
local f = e.DisplayName or e.Name
local g,h,i,j,k,l,m,n,o,p,q,r,s = b,game:GetService('RunService'),game:GetService('UserInputService'),game:GetService('VirtualInputManager'),game:GetService('ReplicatedStorage'),game:GetService('VirtualUser'),game:GetService('Workspace'),game:GetService('HttpService'),game:GetService('StarterPack'),e,g,h,i
_G.w=_G.w or{} if not table.find(_G.w,'Johan') then table.insert(_G.w,'Johan') end
local t,u=false,false
local v=d:AddWindow('CLAN JX7 HUB V3 || Version Public Bienvenido '..f,{main_color=Color3.fromRGB(0,0,255),min_size=Vector2.new(650,870),can_resize=true})
local w,x,y,z,A,B,C,D,E=v:AddTab('Main Op'),v:AddTab('Farm Op'),v:AddTab('Misc'),v:AddTab('Pets'),v:AddTab('Stats'),v:AddTab('Calculator Op'),v:AddTab('Op kills'),v:AddTab('TP AREAS'),v:AddTab('Credits')
local F=w:AddFolder('  Auto Brawls')
F:AddSwitch('God Mode (Brawl)',function(G)t=G;if G then task.spawn(function()while t do k.rEvents.brawlEvent:FireServer('joinBrawl') task.wait(.1)end end)end end)
local H={}
F:AddSwitch('Auto Win Brawls',function(I)getgenv().autoWinBrawl=I
local function J()
    if not getgenv().autoWinBrawl then return end
    local K=c.Character
    if not K then return false end
    if K:FindFirstChild('Punch') then return true end
    local L=c.Backpack;if not L then return false end
    for _,M in pairs(L:GetChildren())do if M.ClassName=='Tool'and M.Name=='Punch'then M.Parent=K return true end end
    return false
end
local function N(O)
    if not O or not O.Parent then return false end
    if O==g.LocalPlayer then return false end
    if _G.w and table.find(_G.w,O.Name)then return false end
    local P=O.Character
    if not P or not P.Parent then return false end
    local Q=P:FindFirstChild('Humanoid')
    if not Q then return false end
    if not Q.Health or Q.Health<=0 then return false end
    if Q:GetState()==Enum.HumanoidStateType.Dead then return false end
    local R=P:FindFirstChild('HumanoidRootPart')
    if not R or not R.Parent then return false end
    return true
end
local function S()
    local T=g.LocalPlayer
    if not T then return false end
    local U=T.Character
    if not U or not U.Parent then return false end
    local V=U:FindFirstChild('Humanoid')
    if not V or V.Health<=0 then return false end
    local W,Ux=U:FindFirstChild('LeftHand'),U:FindFirstChild('RightHand')
    return (W~=nil or Ux~=nil)
end
local function Y(Z,a0)
    if not Z or not Z.Parent then return false end
    if not a0 or not a0.Parent then return false end
    local ok=pcall(function()
        firetouchinterest(Z,a0,0)task.wait(.01)firetouchinterest(Z,a0,1)
    end)
    return ok
end
if I then
    task.spawn(function()
        while getgenv().autoWinBrawl and task.wait(.5)do if not getgenv().autoWinBrawl then break end
            pcall(function()
                if g.LocalPlayer.PlayerGui.gameGui.brawlJoinLabel.Visible then
                    k.rEvents.brawlEvent:FireServer('joinBrawl')
                    g.LocalPlayer.PlayerGui.gameGui.brawlJoinLabel.Visible=false
                end
            end)
        end
    end)
    task.spawn(function()
        while getgenv().autoWinBrawl and task.wait(.5) do if not getgenv().autoWinBrawl then break end J() end
    end)
    task.spawn(function()
        while getgenv().autoWinBrawl and task.wait(.1) do if not getgenv().autoWinBrawl then break end
            if S() and k.brawlInProgress.Value then
                local T=g.LocalPlayer
                pcall(function() T.muscleEvent:FireServer('punch','rightHand') end)
                pcall(function() T.muscleEvent:FireServer('punch','leftHand') end)
            end
        end
    end)
    task.spawn(function()
        while getgenv().autoWinBrawl and task.wait(.05)do if not getgenv().autoWinBrawl then break end
            if S() and k.brawlInProgress.Value then
                local U=g.LocalPlayer.Character
                local W=U:FindFirstChild('LeftHand')
                local Ux=U:FindFirstChild('RightHand')
                for _,O in pairs(g:GetPlayers())do if not getgenv().autoWinBrawl then break end
                    pcall(function()
                        if N(O)then
                            local a2=O.Character.HumanoidRootPart
                            if W then Y(a2,W)end
                            if Ux then Y(a2,Ux)end
                        end
                    end)
                    task.wait(.01)
                end
            end
        end
    end)
end
end)
F:AddSwitch('Auto Join Brawls',function(a3)getgenv().autoJoinBrawl=a3;if a3 then task.spawn(function()while getgenv().autoJoinBrawl and task.wait(.5)do if not getgenv().autoJoinBrawl then break end pcall(function()if g.LocalPlayer.PlayerGui.gameGui.brawlJoinLabel.Visible then k.rEvents.brawlEvent:FireServer('joinBrawl')g.LocalPlayer.PlayerGui.gameGui.brawlJoinLabel.Visible=false end end)end end)end end)
